    "# df['ABV'].value_counts(dropna=False).sort_index() # The only non-number/non-NaN value is '???'\n",
    "df['ABV(float)'] = df['ABV'].str.replace('%', '').replace('???', float('NaN')).astype(float)\n",
    "df.head()"
